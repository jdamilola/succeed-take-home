{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "cross-env NODE_PATH=. ts-node-dev --transpile-only --require dotenv/config index.ts",
    "start": "cross-env NODE_PATH=dist node dist/index.js",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm *.tsbuildinfo",
    "test": "jest --setupFiles dotenv/config",
    "lint": "eslint . --quiet",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "cross-env NODE_PATH=. ts-node-dev --transpile-only --require dotenv/config ./src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "cross-env NODE_PATH=. ts-node-dev --transpile-only --require dotenv/config ./src/db/seed.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.15",
    "@types/express": "4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/uuid": "^9.0.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.4",
    "express": "4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "tsup": "6.6.3",
    "typescript": "4.9.4",
    "ts-node-dev": "2.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "@succeed/logger": "workspace:*",
    "@succeed/tsconfig": "workspace:*",
    "@succeed/eslint": "workspace:*"
  },
  "devDependencies": {
    "@swc/core": "1.11.24",
    "@swc/helpers": "0.5.17",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.10.7",
    "dotenv": "16.0.3",
    "drizzle-kit": "^0.20.14",
    "regenerator-runtime": "0.13.11"
  },
  "keywords": [],
  "author": "Riley Brown",
  "license": "MIT"
}
